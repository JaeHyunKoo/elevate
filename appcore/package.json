{
    "name": "elevate-app",
    "license": "MIT",
    "scripts": {
        "ng": "ng",
        "start": "ng serve",
        "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
        "build": "ng build",
        "test": "ng test --watch=false --code-coverage",
        "test:no-source-map": "npm run test -- --source-map=false",
        "test:ui-browser": "npm run test -- --browsers Chrome",
        "type:check": "tsc --noEmit --project tsconfig.app.json && tsc --noEmit --project tsconfig.spec.json",
        "lint": "ng lint app --fix",
        "e2e": "ng e2e",
        "upgrade:check": "npx ncu",
        "upgrade:apply": "npm run upgrade:check -- -u"
    },
    "config": {
        "unsafe-perm": true
    },
    "private": true,
